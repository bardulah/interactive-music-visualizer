# Spotify Integration
# Get your credentials from: https://developer.spotify.com/dashboard
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id_here

# SoundCloud Integration (Optional)
# Get your credentials from: https://developers.soundcloud.com/
VITE_SOUNDCLOUD_CLIENT_ID=your_soundcloud_client_id_here

# Application Settings
VITE_APP_NAME=Music Visualizer Pro
VITE_APP_VERSION=2.0.0

# Analytics (Optional)
# VITE_ANALYTICS_ID=your_analytics_id_here

# Sentry Error Tracking (Optional)
# VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_3D=true
VITE_ENABLE_SHADERS=true
VITE_ENABLE_MIDI=true
VITE_ENABLE_STREAMING=true
VITE_ENABLE_EFFECTS=true
